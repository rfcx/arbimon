{
  "name": "rfcx-bio",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build-ci": "pnpm -r build",
    "clean-slate": "pnpm add -DW rimraf && pnpm -r clean && pnpm -r --filter {\"packages\"} exec rimraf \"node_modules\" && rimraf \"node_modules\"",
    "init-vscode": "code --force --install-extension dbaeumer.vscode-eslint && code --force --install-extension esbenp.prettier-vscode && code --force --install-extension stylelint.vscode-stylelint && code --force --install-extension johnsoncodehk.volar && code --force --install-extension voorjaar.windicss-intellisense && code --force --install-extension hbenl.vscode-test-explorer && code --force --install-extension kavod-io.vscode-jest-test-adapter",
    "lint:eslint": "eslint -c .eslintrc.js --cache $(git diff --name-only --diff-filter=ACMRTUXB origin/develop HEAD | grep -E \"\\.(js|ts|vue)$\" | xargs)",
    "lint:prettier": "prettier \"./*.{json,md,yaml}\" \"build/**/*.md\" --check",
    "lint": "run-s lint:*",
    "lint-ci": "pnpm -r --parallel --no-bail lint",
    "lint-fix": "eslint \"./*.{ts,js}\" \"./.*.{ts,js}\" --fix && prettier \"./*.{json,md,yaml}\" \"build/**/*.md\" --loglevel warn --write",
    "test": "jest src",
    "serve": "pnpm serve-website",
    "serve-website": "cd packages/website && pnpm serve"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@mixer/parallel-prettier": "^2.0.1",
    "@types/estree": "^0.0.50",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.6",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-vue": "1.8.0",
    "@vue/compiler-sfc": "^3.2.21",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^9.0.1",
    "babel-jest": "^24.9.0",
    "cross-env": "^7.0.3",
    "cypress": "^8.7.0",
    "cypress-fail-on-console-error": "^2.1.2",
    "cypress-wait-until": "^1.7.2",
    "dotenv": "^10.0.0",
    "eslint": "^8.1.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-regex": "^1.8.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-class-members": "^1.14.0",
    "eslint-plugin-unicorn": "^38.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.3.1",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "stylelint": "^14.0.1",
    "stylelint-config-recommended-scss": "^5.0.1",
    "stylelint-scss": "^4.0.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4",
    "vite": "^2.6.13",
    "vite-plugin-style-import": "^1.3.0",
    "vite-plugin-windicss": "^1.5.1",
    "vite-tsconfig-paths": "^3.3.17",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "0.28.7",
    "webpack": "^5.61.0"
  },
  "description": "Monorepo root",
  "homepage": "https://github.com/rfcx/biodiversity-analytics#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rfcx/biodiversity-analytics.git"
  },
  "bugs": {
    "url": "https://github.com/rfcx/biodiversity-analytics/issues"
  },
  "keywords": [
    "biodiversity",
    "analytics",
    "rfcx",
