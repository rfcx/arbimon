{
  "name": "rfcx-bio",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "ci-build": "pnpm -r build",
    "ci-lint": "pnpm -r lint --stream --workspace-concurrency 1",
    "ci-test": "pnpm -r test",
    "clean-sheet": "pnpm clean-slate",
    "clean-slate": "pnpm add -DW rimraf && pnpm -r clean && pnpm -r --filter=./apps --filter=./packages exec rimraf \"node_modules\" && rimraf \"node_modules\"",
    "init-vscode": "code --force --install-extension dbaeumer.vscode-eslint && code --force --install-extension esbenp.prettier-vscode && code --force --install-extension stylelint.vscode-stylelint && code --force --install-extension johnsoncodehk.volar && code --force --install-extension voorjaar.windicss-intellisense && code --force --install-extension hbenl.vscode-test-explorer && code --force --install-extension kavod-io.vscode-jest-test-adapter",
    "lint:eslint": "eslint . --ignore-pattern \"*/\" --cache",
    "lint:prettier": "prettier . --check",
    "lint": "concurrently -r -m 1 --kill-others-on-fail \"pnpm:lint:*\"",
    "lint-diff:eslint": "eslint --cache $(git diff --name-only --diff-filter=ACMRTUXB origin/staging HEAD | grep -E \"\\.(js|cjs|ts|vue)$\" | xargs)",
    "lint-diff": "concurrently -r -m 1 --kill-others-on-fail \"pnpm:lint-diff:*\"",
    "lint-fix": "eslint . --fix --ignore-pattern \"*/\" && prettier . --write --loglevel warn",
    "profile-eslint": "cross-env TIMING=1 eslint . --ignore-pattern \"*/\"",
    "s:api": "pnpm --filter=api serve",
    "s:website": "wait-port -o silent 3000 && pnpm --filter=website serve",
    "sb:db": "pnpm --filter=db serve && pnpm --filter=cli serve src/db-migrations",
    "serve": "concurrently --kill-others-on-fail \"pnpm:s:*\"",
    "serve-beta": "concurrently --kill-others-on-fail \"pnpm:s:*\" \"pnpm:sb:*\""
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@mixer/parallel-prettier": "^2.0.1",
    "@types/estree": "^0.0.50",
    "@types/jest": "^27.0.3",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.6",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vitest/ui": "^0.0.132",
    "@vue/compiler-sfc": "^3.2.21",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^9.0.1",
    "@vue/test-utils": "^2.0.0-rc.16",
    "babel-jest": "^27.4.4",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "cypress": "^8.7.0",
    "cypress-fail-on-console-error": "^2.1.2",
    "cypress-wait-until": "^1.7.2",
    "detect-browsers": "^5.0.5",
    "dotenv": "^10.0.0",
    "eslint": "^8.1.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-regex": "^1.8.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-class-members": "^1.14.0",
    "eslint-plugin-unicorn": "^38.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.4.4",
    "nodemon": "^2.0.15",
    "open-cli": "^7.0.1",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "stylelint": "^14.0.1",
    "stylelint-config-property-sort-order-smacss": "^8.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.7",
    "vite-plugin-style-import": "^1.3.0",
    "vite-plugin-windicss": "^1.5.1",
    "vite-tsconfig-paths": "^3.3.17",
    "vitest": "^0.0.132",
    "vue": "^3.2.21",
    "vue-tsc": "^0.28.10",
    "vue3-jest": "^27.0.0-alpha.1",
    "wait-port": "^0.2.9",
    "webpack": "^5.61.0"
  },
  "description": "Monorepo root",
  "homepage": "https://github.com/rfcx/biodiversity-analytics#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rfcx/biodiversity-analytics.git"
  },
  "bugs": {
    "url": "https://github.com/rfcx/biodiversity-analytics/issues"
  },
  "keywords": [
    "biodiversity",
    "analytics",
    "rfcx",
    "rainforest",
    "connection"
  ]
}
