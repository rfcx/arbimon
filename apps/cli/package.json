{
  "name": "@rfcx-bio/cli",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf \"./lib\" \"./*.tsbuildinfo\" \"./.eslintcache\"",
    "db-migrate": "pnpm serve lib/db/migrate",
    "db-reset": "pnpm clean && pnpm serve lib/db/reset",
    "db-rms": "pnpm serve lib/db/reset-migrate-seed",
    "db-rms-test": "cross-env BIO_DB_PORT=5434 pnpm serve lib/db/reset-migrate-seed -- --path=02-arbimon-data-mock,03-preload,04-user-data-mock,01-integration-test-data",
    "db-seed": "pnpm serve lib/db/seed -- --path=02-arbimon-data-mock,03-preload,04-user-data-mock,01-integration-test-data",
    "lint:eslint": "eslint . --cache",
    "lint:prettier": "prettier . --check",
    "lint": "concurrently -r -m 1 --kill-others-on-fail \"pnpm:lint:*\"",
    "lint-fix": "eslint . --fix && prettier . --loglevel warn --write",
    "profile-eslint": "cross-env TIMING=1 eslint .",
    "serve": "pnpm build && node --experimental-specifier-resolution=node",
    "test-int": "cross-env BIO_DB_PORT=5434 vitest --no-threads --passWithNoTests .int.test.",
    "test-int-ci": "vitest --no-threads --passWithNoTests .int.test.",
    "test-unit": "vitest --passWithNoTests .unit.test.",
    "test-unit-coverage": "vitest --coverage --passWithNoTests .unit.test."
  },
  "dependencies": {
    "@rfcx-bio/common": "workspace:^0.0.1",
    "@rfcx-bio/utils": "workspace:^0.0.1",
    "axios": "^0.24.0",
    "dayjs": "^1.10.7",
    "dedent": "^0.7.0",
    "dotenv": "^10.0.0",
    "lodash-es": "^4.17.21",
    "mysql2": "^2.3.3",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "7.0.0-alpha.5",
    "umzug": "^3.0.0"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/mysql": "^2.15.19",
    "@types/object-hash": "^2.2.1",
    "@types/validator": "^13.7.0",
    "object-hash": "^3.0.0",
    "sqlite3": "^5.0.2"
  },
  "description": "Biodiversity CLI",
  "homepage": "https://github.com/rfcx/biodiversity-analytics#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rfcx/biodiversity-analytics.git"
  },
  "bugs": {
    "url": "https://github.com/rfcx/biodiversity-analytics/issues"
  },
  "keywords": [
    "biodiversity",
    "analytics",
    "rfcx",
    "rainforest",
    "connection"
  ]
}
