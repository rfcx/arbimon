{
  "name": "@rfcx-bio/cli",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf \"./lib\" \"./*.tsbuildinfo\"",
    "db-migrate": "pnpm serve lib/db/migrate",
    "db-reset": "pnpm clean && pnpm serve lib/db/reset",
    "db-seed": "pnpm serve lib/db/seed -- --path=auto",
    "lint:eslint": "eslint . --cache",
    "lint:prettier": "prettier . --check",
    "lint": "concurrently -r -m 1 --kill-others-on-fail \"pnpm:lint:*\"",
    "lint-fix": "eslint . --fix && prettier . --loglevel warn --write",
    "profile-eslint": "cross-env TIMING=1 eslint .",
    "serve": "pnpm build && node --experimental-specifier-resolution=node",
    "test": "vitest --run --passWithNoTests",
    "test-coverage": "vitest --run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@rfcx-bio/common": "workspace:^0.0.1",
    "@rfcx-bio/utils": "workspace:^0.0.1",
    "axios": "^0.24.0",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "lodash-es": "^4.17.21",
    "mysql2": "^2.3.3",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "7.0.0-alpha.5",
    "umzug": "^3.0.0"
  },
  "devDependencies": {
    "@types/mysql": "^2.15.19",
    "@types/validator": "^13.7.0"
  },
  "description": "Biodiversity CLI",
  "homepage": "https://github.com/rfcx/biodiversity-analytics#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rfcx/biodiversity-analytics.git"
  },
  "bugs": {
    "url": "https://github.com/rfcx/biodiversity-analytics/issues"
  },
  "keywords": [
    "biodiversity",
    "analytics",
    "rfcx",
    "rainforest",
    "connection"
  ]
}
