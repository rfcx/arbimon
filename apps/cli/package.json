{
  "name": "@rfcx-bio/cli",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf \"./lib\" \"./*.tsbuildinfo\"",
    "lint:eslint": "eslint . --cache",
    "lint:prettier": "prettier . --check",
    "lint": "concurrently -r -m 1 --kill-others-on-fail \"pnpm:lint:*\"",
    "lint-fix": "eslint . --fix && prettier . --loglevel warn --write",
    "profile-eslint": "cross-env TIMING=1 eslint .",
    "serve": "cross-env TS_NODE_PROJECT=\"tsconfig.build.json\" node --experimental-specifier-resolution=node --loader ./loader.js",
    "test": "vitest --run --passWithNoTests",
    "test-coverage": "vitest --run --coverage --passWithNoTests"
  },
  "active-issues": {
    "sequelize": "6.12.0 breaks build - https://github.com/sequelize/sequelize/issues/13791"
  },
  "dependencies": {
    "@rfcx-bio/common": "workspace:*",
    "axios": "^0.24.0",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "lodash-es": "^4.17.21",
    "mysql2": "^2.3.3",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "6.12.0-beta.3",
    "umzug": "^3.0.0"
  },
  "devDependencies": {
    "@types/mysql": "^2.15.19",
    "@types/validator": "^13.7.0"
  },
  "description": "Biodiversity CLI",
  "homepage": "https://github.com/rfcx/biodiversity-analytics#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rfcx/biodiversity-analytics.git"
  },
  "bugs": {
    "url": "https://github.com/rfcx/biodiversity-analytics/issues"
  },
  "keywords": [
    "biodiversity",
    "analytics",
    "rfcx",
    "rainforest",
    "connection"
  ]
}
