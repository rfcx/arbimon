{
  "name": "@rfcx-bio/website",
  "version": "0.0.1",
  "private": true,
  "description": "Analyze biodiversity data from the RFCx platform",
  "keywords": [
    "biodiversity",
    "analytics",
    "rfcx",
    "rainforest",
    "connection"
  ],
  "homepage": "https://github.com/rfcx/biodiversity-analytics#readme",
  "bugs": {
    "url": "https://github.com/rfcx/biodiversity-analytics/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rfcx/biodiversity-analytics.git"
  },
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "vue-tsc --project \"tsconfig.vuetsc.json\" --noEmit --skipLibCheck && vite build",
    "clean": "rimraf \"./dist\" \"./.eslintcache\" \"./.stylelintcache\"",
    "lint": "concurrently -r -m 1 --kill-others-on-fail \"pnpm:lint:*\"",
    "lint-fix": "eslint . --fix && prettier . --loglevel warn --write && stylelint . --fix",
    "lint-profile:eslint": "cross-env TIMING=1 NODE_OPTIONS=\"--max_old_space_size=8192\" eslint .",
    "lint:eslint": "eslint . --cache",
    "lint:prettier": "prettier . --check",
    "lint:stylelint": "stylelint . --cache",
    "open": "wait-port -o silent localhost:8101 && node ./launch.js",
    "preview": "vite preview --port 8101",
    "serve": "concurrently -r \"vite\" \"pnpm open\"",
    "test": "concurrently -r -m 1 --kill-others-on-fail \"pnpm:test:* -- --run\"",
    "test-ui": "cypress run",
    "test-ui-explorer": "cypress open",
    "test:component": "vitest .vue.test.",
    "test:unit": "vitest .unit.test."
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.19.2",
    "@rfcx-bio/common": "workspace:^0.0.1",
    "@rfcx-bio/utils": "workspace:^0.0.1",
    "@types/geojson": "^7946.0.10",
    "@types/markdown-it": "^12.2.3",
    "@vueuse/components": "^6.7.6",
    "@vueuse/core": "^6.7.6",
    "axios": "^0.27.2",
    "d3": "^7.1.1",
    "dayjs": "^1.10.7",
    "element-plus": "^2.2.2",
    "howler": "^2.2.3",
    "jszip": "^3.7.1",
    "lodash-es": "^4.17.21",
    "mapbox-gl": "^2.5.1",
    "numeral": "^2.0.6",
    "pinia": "^2.0.2",
    "vue": "^3.2.21",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-gtag": "^2.0.1",
    "vue-property-decorator": "^10.0.0-rc.3",
    "vue-query": "^1.22.3",
    "vue-router": "^4.0.12",
    "vue3-markdown-it": "^1.0.9",
    "xlsx": "^0.17.3"
  },
  "devDependencies": {
    "@iconify-json/fa": "^1.0.1",
    "@iconify-json/fa-regular": "^1.0.2",
    "@iconify-json/fa-solid": "^1.0.2",
    "@pinia/testing": "^0.0.12",
    "@types/color-string": "^1.5.0",
    "@types/d3": "^7.1.0",
    "@types/howler": "^2.2.4",
    "@types/mapbox-gl": "^2.4.2",
    "@types/numeral": "^2.0.2",
    "@windicss/plugin-animations": "^1.0.9",
    "@windicss/plugin-interaction-variants": "^1.0.0",
    "c8": "^7.10.0",
    "happy-dom": "^5.3.1",
    "unplugin-auto-import": "^0.8.7",
    "unplugin-icons": "^0.12.22",
    "unplugin-vue-components": "^0.19.6",
    "windicss": "^3.2.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
