{
  "name": "@rfcx-bio/website",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "clean": "rimraf \"./dist\"",
    "lint:types": "vue-tsc --project \"tsconfig.json\" --noEmit --skipLibCheck",
    "lint:eslint": "eslint . --cache",
    "lint:prettier": "prettier . --check",
    "lint:stylelint": "stylelint . --cache",
    "lint": "concurrently -r -m 1 --kill-others-on-fail \"pnpm:lint:*\"",
    "lint-fix": "eslint . --fix && prettier . --loglevel warn --write && stylelint . --fix",
    "preview": "vite preview --port 8080",
    "profile-eslint": "cross-env TIMING=1 NODE_OPTIONS=\"--max_old_space_size=8192\" eslint .",
    "serve": "vite",
    "test-ui:explorer": "cypress open"
  },
  "active-issues": {
    "@vitejs/plugin-vue": "1.8.1 breaks vue-class-component - https://github.com/vitejs/vite/pull/5374",
    "vue-tsc": "0.28.8 breaks website build - https://github.com/johnsoncodehk/volar/issues/670"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.19.2",
    "@iconify-json/fa-regular": "^1.0.2",
    "@rfcx-bio/common": "workspace:*",
    "@vueuse/components": "^6.7.6",
    "@vueuse/core": "^6.7.6",
    "axios": "^0.24.0",
    "d3": "^7.1.1",
    "dayjs": "^1.10.7",
    "element-plus": "^1.2.0-beta.5",
    "jszip": "^3.7.1",
    "lodash-es": "^4.17.21",
    "mapbox-gl": "^2.5.1",
    "numeral": "^2.0.6",
    "pinia": "^2.0.2",
    "vue": "^3.2.21",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-property-decorator": "^10.0.0-rc.3",
    "vue-router": "^4.0.12",
    "vue3-markdown-it": "^1.0.9",
    "xlsx": "^0.17.3"
  },
  "devDependencies": {
    "@iconify-json/fa": "^1.0.1",
    "@iconify-json/fa-solid": "^1.0.2",
    "@types/color-string": "^1.5.0",
    "@types/d3": "^7.1.0",
    "@types/mapbox-gl": "^2.4.2",
    "@types/numeral": "^2.0.2",
    "@windicss/plugin-animations": "^1.0.9",
    "@windicss/plugin-interaction-variants": "^1.0.0",
    "unplugin-auto-import": "^0.5.1",
    "unplugin-icons": "^0.12.22",
    "unplugin-vue-components": "^0.17.8",
    "windicss": "^3.2.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "description": "Analyze biodiversity data from the RFCx platform",
  "homepage": "https://github.com/rfcx/biodiversity-analytics#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rfcx/biodiversity-analytics.git"
  },
  "bugs": {
    "url": "https://github.com/rfcx/biodiversity-analytics/issues"
  },
  "keywords": [
    "biodiversity",
    "analytics",
    "rfcx",
    "rainforest",
    "connection"
  ]
}
