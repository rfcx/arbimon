  "private": true,
  "description": "Biodiversity DB",
  "keywords": [
    "biodiversity",
    "analytics",
    "rfcx",
    "rainforest",
    "connection"
  ],
  "homepage": "https://github.com/rfcx/arbimon#readme",
  "bugs": {
    "url": "https://github.com/rfcx/arbimon/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rfcx/arbimon.git"
  },
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "logs": "docker compose logs timescaledb",
    "reset": "docker compose down && docker volume rm db_data-timescaledb",
    "serve": "docker compose up -d --wait",
    "serve-int": "docker compose -f docker-compose.test.yaml up -d --quiet-pull --wait && docker compose exec -T -e PGPASSWORD=test timescaledb-test pg_isready",
    "stop": "docker compose down",
    "stop-int": "docker compose -f docker-compose.test.yaml down"
  }
}
